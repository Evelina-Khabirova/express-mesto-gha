{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
      }
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error", {
          "allow": ["_id"]
        }
      ]
    }
}
